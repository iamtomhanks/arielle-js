{
  "name": "arielle-js",
  "version": "0.1.0",
  "description": "CLI for ArielleJS - Your OpenAPI companion",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "arielle": "./dist/index.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prepare": "npm run build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch"
  },
  "keywords": [
    "openapi",
    "cli",
    "documentation",
    "nlp"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@chroma-core/default-embed": "^0.1.8",
    "@google/generative-ai": "^0.24.1",
    "@types/js-yaml": "^4.0.9",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "chromadb": "^3.0.15",
    "commander": "^11.1.0",
    "gradient-string": "^3.0.0",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0",
    "mustache": "^4.2.0",
    "openai": "^5.20.2",
    "openapi-types": "^12.1.3",
    "ora": "^8.0.1",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
